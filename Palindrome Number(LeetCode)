class Solution {
    public boolean isPalindrome(int x) {
        
        int i,j;
        String s = Integer.toString(x);
        
        for(i=0,j=s.length()-1;i<j;i++,j--)
        {
            if(s.charAt(i) != s.charAt(j))
               return false;     
        }
        return true;
    }
}
